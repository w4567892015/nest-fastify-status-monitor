    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/websockets": "^10.3.8",
    "axios": "^0.19.2",
    "debug": "^2.6.8",
    "event-loop-stats": "^1.4.1",
    "handlebars": "4.7.3",
